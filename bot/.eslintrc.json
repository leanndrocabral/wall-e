{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["google"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "require-jsdoc" : 0,
        "camelcase": ["error", {"allow": ["temp_f", "temp_c", "max_tokens"]}]
    }
}
